{
  "name": "mrgarest/echo-api",
  "description": "A simple package for generating API responses in Laravel",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0",
  "homepage": "https://github.com/mrgarest/echo-api",
  "keywords": [
    "garest",
    "laravel",
    "json",
    "api",
    "response",
    "php"
  ],
  "authors": [
    {
      "name": "Garest",
      "email": "olehhoncharenko98@gmail.com",
      "homepage": "https://github.com/mrgarest"
    }
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "MrGarest\\EchoApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MrGarest\\EchoApi\\EchoApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
